[
	fun -> dispatch function
	env -> enviroment reference
	arg... arguments/properties
]

call(len, env, ...arg)
	fun : fun[0](len, fun[1], ...arg)
	obj : obj[0](len, obj[1], ...arg)
	str :
	int :
	flt :
	var : box[0](len, env, ...arg)

box[fun, env, ...box] interned (strings, numbers, booleans)
obj[fun, env, ...obj] instatianted (closures, objects, classes)


// function structure
i64 fun_id(i64 argc/* argument count */, p64 argv/* argument vector */, p64 argx/* argument context */) {
	do {
		// function procedure
	} while (0);

	return rax; // function return most recent evaulated expression
}
